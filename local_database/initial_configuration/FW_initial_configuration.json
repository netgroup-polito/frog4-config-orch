{
	"config-firewall:interfaces": {
		"ifEntry": [	
			{
				"id": "L3Port:0",
				"name": "eth0",
				"type": "L3",
				"management": true,
				"ipv4_configuration": {
					"configurationType": "dhcp"
				}
			},
			{
				"id": "L2Port:1",
				"name": "eth1",
				"type": "transparent"
			},
			{
				"id": "L2Port:2",
				"name": "eth2",
				"type": "transparent"
			}
		]
	},
	"config-firewall:firewall": {
		"wan-interface": "eth2",
		"policies": [
			{
			"policy-name": "pol1",
			"description": "allow ssh",
			"action": "accept",
			"protocol": "tcp",
			"destination-port": "22"
			},
			{
			"policy-name": "pol2",
			"description": "allow incoming http traffic",
			"action": "accept",
			"protocol": "tcp",
			"source-port": "80",
			"in-interface": "eth1"
			},
			{
			"policy-name": "pol3",
			"description": "allow outgoing http traffic",
			"action": "accept",
			"protocol": "tcp",
			"destination-port": "80",
			"out-interface": "eth2"
			},
			{
			"policy-name": "pol4",
			"description": "allow incoming https traffic",
			"action": "accept",
			"protocol": "tcp",
			"source-port": "443",
			"in-interface": "eth1"
			},
			{
			"policy-name": "pol5",
			"description": "allow outgoing https traffic",
			"action": "accept",
			"protocol": "tcp",
			"destination-port": "443",
			"out-interface": "eth2"
			},
			{
			"policy-name": "pol6",
			"description": "allow incoming request dns ",
			"action": "accept",
			"protocol": "udp",
			"source-port": "53",
			"in-interface": "eth1"
			},
			{
			"policy-name": "pol7",
			"description": "allow outgoing request dns",
			"action": "accept",
			"protocol": "udp",
			"destination-port": "53",
			"out-interface": "eth2"
			},
			{
			"policy-name": "pol8",
			"description": "block all outgoing traffic",
			"action": "drop",
			"protocol": "all",
			"out-interface": "eth2"
			}
		],
		"blacklist" : [
			{
				"url": "www.wikipedia.com"
			},
			{
				"url": "www.youtube.com"
			}
		],
		"whitelist" : []
	}
} 
